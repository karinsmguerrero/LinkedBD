<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<MenuBar xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="GUI.MainWindowController"
            prefHeight="400.0" prefWidth="600.0">
<menus>
    <Menu
            mnemonicParsing="false"
            text="Nuevo...">
        <items>
            <MenuItem
                    fx:id="newTableWindow"
                    mnemonicParsing="false"
                    onAction="#newTableWindow"
                    text="Nueva tabla" />
        </items>

        <items>
            <MenuItem
                    fx:id="newDBWindow"
                    mnemonicParsing="false"
                    onAction="#newDBWindow"
                    text="Nueva base de datos" />
        </items>
    </Menu>
</menus>
</MenuBar>
